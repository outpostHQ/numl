{
  "name": "nude-elements",
  "version": "0.2.1",
  "module": "dist/nude.js",
  "devDependencies": {
    "@babel/core": "7.2.0",
    "css-color-function": "^1.3.3",
    "cssnano": "^4.1.10",
    "inline-assets": "^1.4.3",
    "install": "^0.13.0",
    "npm": "^6.10.2",
    "parcel-bundler": "^1.6.1",
    "rollup": "^1.0.0",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "size-limit": "^1.3.7"
  },
  "scripts": {
    "build": "rollup -c && size-limit",
    "dev": "rollup -c -w",
    "play": "rm test/* && parcel --out-dir test test.html --open",
    "size": "size-limit"
  },
  "files": [
    "dist"
  ],
  "size-limit": [
    {
      "name": "nude",
      "webpack": false,
      "path": [
        "dist/nude.js"
      ]
    },
    {
      "name": "nude pack",
      "webpack": false,
      "path": [
        "dist/nude.pack.js",
        "dist/nude.pack.css"
      ]
    }
  ]
}
